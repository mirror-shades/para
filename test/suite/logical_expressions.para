const x = 5
const y = 10
const z = 5.5
const w = 10.5
const name1 = "hello"
const name2 = "world"
const flag1 = true
const flag2 = false

# Integer comparisons
const int_lt = x < y
const int_gt = x > y
const int_eq = x == 5
const int_neq = x != y
const int_lte = x <= 5
const int_gte = y >= 10

# Float comparisons
const float_lt = z < w
const float_gt = w > z
const float_eq = z == 5.5
const float_neq = z != w

# String comparisons
const str_eq = name1 == "hello"
const str_neq = name1 != name2

# Bool comparisons
const bool_eq = flag1 == true
const bool_neq = flag1 != flag2

# Logical NOT
const not_true = !flag2
const not_false = !flag1

# Logical AND operations
const and_true_true = flag1 and flag1
const and_true_false = flag1 and flag2
const and_false_true = flag2 and flag1
const and_false_false = flag2 and flag2

# Logical OR operations
const or_true_true = flag1 or flag1
const or_true_false = flag1 or flag2
const or_false_true = flag2 or flag1
const or_false_false = flag2 or flag2

# Complex logical expressions
const complex_and = (x < y) and (flag1 == true)
const complex_or = (x > y) or (flag2 == false)
const chained_logic = flag1 and flag2 or flag1

# Mixed type comparisons (should work for same types)
const mixed_int = x == y
const mixed_float = z == w

